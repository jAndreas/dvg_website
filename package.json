{
	"name": "dvg-website",
	"version": "0.0.1",
	"description": "Website of project Der Vegane Germane",
	"main": "app.js",
	"scripts": {
		"legacy": "webpack --mode production --progress --config webpack.legacy",
		"dev": "webpack --mode development --watch --progress --config webpack.dev",
		"prod": "webpack --mode production --progress --config webpack.prod"
	},
	"author": "Andreas Goebel",
	"license": "MIT",
	"dependencies": {
		"@babel/core": "^7.6.2",
		"@babel/plugin-proposal-class-properties": "^7.5.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-destructuring": "^7.5.0",
		"@babel/plugin-transform-regenerator": "^7.4.5",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/polyfill": "^7.4.4",
		"@babel/preset-env": "^7.6.2",
		"@babel/runtime": "^7.6.2",
		"@babel/runtime-corejs2": "^7.6.2",
		"babel-eslint": "^10.0.2",
		"babel-loader": "^8.0.6",
		"clipboard": "^2.0.4",
		"css-loader": "^3.0.0",
		"eslint": "^6.0.1",
		"eslint-loader": "^3.0.2",
		"file-loader": "^4.0.0",
		"hls.js": "^0.12.4",
		"loader-utils": "^1.2.3",
		"node-sass": "^4.12.0",
		"proxy-polyfill": "^0.3.0",
		"raw-loader": "^3.0.0",
		"sass-loader": "^8.0.0",
		"socket.io": "^2.3.0",
		"style-loader": "^1.0.0",
		"uglify-js": "^3.6.0",
		"url-loader": "^2.0.1",
		"url-search-params": "^1.1.0",
		"webpack": "^4.41.0",
		"webpack-cli": "^3.3.5",
		"webpack-sources": "^1.3.0",
		"whatwg-fetch": "^3.0.0"
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"extends": "eslint:recommended",
		"rules": {
			"indent": [
				"off",
				"tab"
			],
			"linebreak-style": [
				"error",
				"unix"
			],
			"quotes": [
				"error",
				"single"
			],
			"semi": [
				"error",
				"always"
			],
			"no-console": "warn",
			"no-debugger": "warn",
			"no-unused-vars": "warn",
			"no-async-promise-executor": "warn"
		},
		"env": {
			"browser": true,
			"node": true,
			"es6": true
		},
		"parserOptions": {
			"ecmaVersion": 7,
			"sourceType": "module",
			"allowImportExportEverywhere": true
		},
		"globals": {
			"ENV_PROD": false,
			"ENV_PUBLIC_PATH": "readonly"
		}
	}
}

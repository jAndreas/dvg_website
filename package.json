{
	"name": "dvg-website",
	"version": "0.0.1",
	"description": "Website of project Der Vegane Germane",
	"main": "app.js",
	"scripts": {
		"legacy": "webpack --mode production --progress --config webpack.legacy",
		"dev": "webpack --mode development --watch --progress --config webpack.dev",
		"prod": "webpack --mode production --progress --config webpack.prod"
	},
	"author": "Andreas Goebel",
	"license": "MIT",
	"dependencies": {
		"@babel/core": "^7.13.1",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-destructuring": "^7.13.0",
		"@babel/plugin-transform-regenerator": "^7.12.13",
		"@babel/plugin-transform-runtime": "^7.13.7",
		"@babel/polyfill": "^7.12.1",
		"@babel/preset-env": "^7.13.5",
		"@babel/runtime": "^7.13.7",
		"@babel/runtime-corejs2": "^7.13.7",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.2",
		"clipboard": "^2.0.6",
		"css-loader": "^5.0.2",
		"eslint": "^7.20.0",
		"eslint-loader": "^4.0.2",
		"fibers": "^5.0.0",
		"file-loader": "^6.2.0",
		"hls.js": "^0.14.17",
		"loader-utils": "^2.0.0",
		"node-sass": "^5.0.0",
		"npm-check-updates": "^11.1.9",
		"proxy-polyfill": "^0.3.2",
		"raw-loader": "^4.0.2",
		"sass": "^1.32.8",
		"sass-loader": "^11.0.1",
		"socket.io": "^3.1.1",
		"socket.io-client": "^3.1.1",
		"style-loader": "^2.0.0",
		"uglify-js": "^3.12.8",
		"url-loader": "^4.1.1",
		"url-search-params": "^1.1.0",
		"webpack": "^5.24.2",
		"webpack-cli": "^4.5.0",
		"webpack-sources": "^2.2.0",
		"whatwg-fetch": "^3.6.1"
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"extends": "eslint:recommended",
		"rules": {
			"indent": [
				"off",
				"tab"
			],
			"linebreak-style": [
				"error",
				"unix"
			],
			"quotes": [
				"error",
				"single"
			],
			"semi": [
				"error",
				"always"
			],
			"no-console": "warn",
			"no-debugger": "warn",
			"no-unused-vars": "warn",
			"no-async-promise-executor": "warn"
		},
		"env": {
			"browser": true,
			"node": true,
			"es6": true
		},
		"parserOptions": {
			"ecmaVersion": 7,
			"sourceType": "module",
			"allowImportExportEverywhere": true
		},
		"globals": {
			"ENV_PROD": false,
			"ENV_PUBLIC_PATH": "readonly"
		}
	}
}

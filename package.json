{
	"name": "dvg-website",
	"version": "0.0.1",
	"description": "Website of project Der Vegane Germane",
	"main": "app.js",
	"scripts": {
		"legacy": "webpack --mode production --progress --config webpack.legacy",
		"dev": "webpack --mode development --watch --progress --config webpack.dev",
		"prod": "webpack --mode development --progress --config webpack.prod"
	},
	"author": "Andreas Goebel",
	"license": "MIT",
	"dependencies": {
		"@babel/core": "^7.0.0",
		"@babel/plugin-proposal-class-properties": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-transform-regenerator": "^7.0.0",
		"@babel/plugin-transform-runtime": "^7.0.0",
		"@babel/plugin-transform-destructuring": "^7.0.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/runtime-corejs2": "^7.0.0",
		"babel-eslint": "^9.0.0",
		"babel-loader": "^8.0.0",
		"barfoos2.0": "^0.3.0",
		"clipboard": "^2.0.1",
		"css-loader": "^1.0.0",
		"eslint": "^5.4.0",
		"eslint-loader": "^2.1.0",
		"hls.js": "^0.11.0",
		"loader-utils": "^1.1.0",
		"node-sass": "^4.9.3",
		"proxy-polyfill": "^0.3.0",
		"raw-loader": "^0.5.1",
		"sass-loader": "^7.1.0",
		"socket.io": "^2.1.0",
		"style-loader": "^0.23.0",
		"uglify-js": "^3.4.7",
		"url-loader": "^1.1.1",
		"url-search-params": "^1.0.2",
		"webpack": "^4.16.5",
		"webpack-cli": "^3.1.0",
		"webpack-sources": "^1.2.0"
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"extends": "eslint:recommended",
		"rules": {
			"indent": [
				"off",
				"tab"
			],
			"linebreak-style": [
				"error",
				"unix"
			],
			"quotes": [
				"error",
				"single"
			],
			"semi": [
				"error",
				"always"
			],
			"no-console": "warn",
			"no-debugger": "warn",
			"no-unused-vars": "warn"
		},
		"env": {
			"browser": true,
			"node": true,
			"es6": true
		},
		"parserOptions": {
			"ecmaVersion": 7,
			"sourceType": "module",
			"allowImportExportEverywhere": true
		},
		"globals": {
			"ENV_PROD": false
		}
	},
	"devDependencies": {
		"file-loader": "^2.0.0"
	}
}
